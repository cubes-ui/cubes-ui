{
  "name": "cubes-ui",
  "version": "0.0.63",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/src/index.d.ts",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "bin": {
    "cubes-ui-init": "./bin/cubes-ui-init.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --minify --target browser --external react --external react-dom --external @tanstack/react-query --external @tanstack/react-router --external lucide-react && tsc -p tsconfig.build.json",
    "postbuild": "npm version patch --no-git-tag-version",
    "test": "vitest"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/types/src/index.d.ts"
    },
    "./index.css": {
      "import": "./index.css",
      "default": "./index.css"
    },
    "./dist/index.css": {
      "import": "./dist/index.css",
      "default": "./dist/index.css"
    }
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-router": "^1.129.8",
    "lucide-react": "^0.525.0",
    "sonner": "^2.0.6",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
